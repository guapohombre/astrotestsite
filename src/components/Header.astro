---
import Test from '../../public/favicon.svg'

---




    <header>
        <div class="logo">
            <div class="img-container">
                <img src="/favicon.svg" alt="">
                </div>
            <h1>Header</h1>
    
        </div>
    
        <ul>
            <li><a href="/test/">Go To Test</a></li>
            <li><a href="/laura/">Laura</a></li>
            <li><a href="/">Home</a></li>
        </ul>

        <div class="custom-shape-divider-top-1733875253">
            <svg data-name="Layer 1" xmlns="http://www.w3.org/2000/svg" viewBox="0 0 1200 120" preserveAspectRatio="none">
                <path d="M321.39,56.44c58-10.79,114.16-30.13,172-41.86,82.39-16.72,168.19-17.73,250.45-.39C823.78,31,906.67,72,985.66,92.83c70.05,18.48,146.53,26.09,214.34,3V0H0V27.35A600.21,600.21,0,0,0,321.39,56.44Z" class="shape-fill"></path>
            </svg>
        </div>
    
    </header>





<style>

    .logo {
        display: flex;
    }

    header {
    
        display: flex;
        align-items: center;
        padding-inline: 5rem;
        justify-content: space-between;
     
        color: white;
        background-color: rgba(0, 0, 0, 0.487);
        position:sticky;
        top:0;
    }

    li {
        display: inline;
        margin-right: 4rem;
    }

    .img-container {
        width: 100px;
        height: 100px;
    }

    .custom-shape-divider-top-1733875253 {
    position: absolute;
    top: 100%;
    left: 0;
    width: 100%;
    overflow: hidden;
    line-height: 0;
}

.custom-shape-divider-top-1733875253 svg {
    position: relative;
    display: block;
    width: calc(100% + 1.3px);
    height: 47px;
}

.custom-shape-divider-top-1733875253 .shape-fill {
    fill: rgba(0, 0, 0, 0.487);
}

}

</style>